<idea-plugin>
  <id>com.github.b3er.idea.plugins.arc.browser</id>
  <name>Archive browser</name>
  <version>0.4</version>
  <vendor email="b3er@who.ms" url="https://github.com/b3er/idea-archive-browser">Ilya Usanov
  </vendor>

  <description><![CDATA[
     This plugin allows you to browse zip archives like folders in IDEA.
    ]]></description>

  <change-notes><![CDATA[
    <ul>
      <li>0.4: Fixed formatting.</li>
      <li>0.3: Fixed typo.</li>
      <li>0.2: Added apk, aar extensions.</li>
      <li>0.1: Initial release.</li>
    </ul>
    ]]>
  </change-notes>

  <idea-version since-build="145.0"/>
  <extensions defaultExtensionNs="com.intellij">
    <treeStructureProvider
        implementation="com.github.b3er.idea.plugins.arc.browser.ZipArchiveStructureProvider"/>
    <fileTypeFactory
        implementation="com.github.b3er.idea.plugins.arc.browser.ZipFileTypeFactory"/>
  </extensions>

  <actions>
    <!-- Add your actions here -->
  </actions>

</idea-plugin>